{
  "name": "hpolthof/laravel-backblaze",
  "type": "library",
  "description": "Backblaze B2 Cloud Storage for Laravel 5",
  "keywords": [
    "laravel",
    "B2",
    "flysystem",
    "filesystem",
    "backblaze",
    "disks"
  ],
  "homepage": "https://github.com/hpolthof/laravel-backblaze",
  "license": "MIT",
  "authors": [
    {
      "name": "Paul Olthof",
      "email": "hpolthof@gmail.com",
      "homepage": "https://github.com/hpolthof",
      "role": "Developer"
    }
  ],

  "require": {
    "php": ">=7.0",
    "laravel/framework": "~5.1",
    "mhetreramesh/flysystem-backblaze": "~1"
  },

  "require-dev": {
    "orchestra/testbench": "^3.8"
  },

  "autoload": {
    "psr-4": {
      "Hpolthof\\Backblaze\\": "src"
    }
  },

  "autoload-dev": {
    "psr-4": {
      "Hpolthof\\Backblaze\\Tests\\": "tests/"
    }
  },

  "minimum-stability": "stable",
  "extra": {
    "laravel": {
      "providers": [
        "Hpolthof\\Backblaze\\BackblazeServiceProvider"
      ]
    }
  }
}